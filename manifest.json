{
    "manifest_version": 2,
    "name": "Smart Page",
    "version": "1.0",
    "description": "This extension will give you a smart new tab with daily news summary and much more.",
    "icons":{
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },

    "chrome_url_overrides" : {
        "newtab": "newtab.html"
      },

    "permissions": [
        "storage",
        "notifications"
    ]

}